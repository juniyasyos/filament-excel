{
    "name": "juniyasyos/filament-excel",
    "description": "Supercharged Excel exports for Filament Resources",
    "license": "MIT",
    "keywords": [
        "laravel",
        "phpspreadsheet",
        "phpexcel",
        "filament",
        "laravel-filament",
        "actions"
    ],
    "authors": [
        {
            "name": "Juniyasyos",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0.0-stable",
        "maatwebsite/excel": "^3.1",
        "anourvalar/eloquent-serialize": "^1.2"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "pxlrbtxjuniyasyos\\FilamentExcel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxlrbtxjuniyasyos\\FilamentExcel\\FilamentExcelServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    },
    "prefer-stable": true
}